package hw;
import java.util.Scanner;

public class hw1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scanner = new Scanner(System.in);
		float avenger = 0;
		for(int i = 1; i <= 3; i++){
			avenger += scanner.nextFloat();
        }
		avenger = avenger/3;
		
        System.out.printf("%d", avenger/3);
		
	}

}
